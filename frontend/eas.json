        "EXPO_PUBLIC_BASE_URL": "@EXPO_PUBLIC_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "@EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
      }
    },
    "preview": {
      "env": {
        "EXPO_PUBLIC_BASE_URL": "@EXPO_PUBLIC_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "@EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_BASE_URL": "@EXPO_PUBLIC_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "@EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
      }
    }
  }
}
